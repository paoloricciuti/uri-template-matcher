{
	"name": "uri-template-matcher",
	"version": "1.1.0",
	"description": "A lightweight URI template matcher based on RFC 6570",
	"type": "module",
	"main": "./src/index.js",
	"types": "./src/index.d.ts",
	"files": [
		"src/*"
	],
	"exports": {
		".": {
			"types": "./src/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"scripts": {
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"check": "pnpm typecheck && pnpm lint && pnpm format:check && npx publint",
		"typecheck": "tsc --noEmit",
		"lint": "eslint src test",
		"lint:fix": "eslint src test --fix",
		"format": "prettier --write src test",
		"format:check": "prettier --check src test",
		"release": "changeset publish"
	},
	"keywords": [
		"uri",
		"template",
		"matcher",
		"rfc6570",
		"routing"
	],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@eslint/js": "^9.30.1",
		"@types/node": "^24.0.12",
		"@typescript-eslint/eslint-plugin": "^8.36.0",
		"@typescript-eslint/parser": "^8.36.0",
		"eslint": "^9.30.1",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.1",
		"prettier": "^3.6.2",
		"publint": "^0.3.12",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@changesets/cli": "^2.29.5"
	}
}
